{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "$ALGOLIA_APP_KEY",
    "$ALGOLIA_APP_SECRET",
    "$ROOSONGO_WEB_URL",
    "$STRAPI_WEB_APP_KEY",
    "$ROOSONGO_CLIENT_API_HOST"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
